{
    "manifest_version": 3,
    "name": "Reading Time Chrome Extension",
    "description": "Add the reading time to Chrome Extension documentation articles",
    "version": "1.0",
    "action": {
      "default_popup": "odoodoc-reading-time.html"
    },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://www.odoo.com/*/*/*"
      ]
    }
  ]
  }
  
